---
# Restart services
- hosts: zabbix_client
  remote_user: root
  become: True
  tasks:
    - name: restart multiple services in a loop
      service:
        name: "{{ item }}"
        state: started
        enabled: true
      register: service_output
      loop:
       - zabbix-agent
       - httpd

    - debug: var=service_output
